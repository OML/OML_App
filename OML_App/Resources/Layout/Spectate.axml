      android:layout_marginTop="8dp"
      android:background="@drawable/menubox2">
      <Button
        android:id="@+id/spec_orientbutton"
        android:layout_width="142dp"
        android:layout_height="55dp"
        android:background="@drawable/orientbutton"/>
    </RelativeLayout>
    <ViewFlipper
          android:id="@+id/spectateflipper"
          android:layout_width="640dp"
          android:layout_height="520dp"
          android:layout_marginLeft="8dp"
          android:layout_marginTop="8dp"
          android:layout_toRightOf="@id/buttonbox0">
      <RelativeLayout
          android:id="@+id/spec_midbox"
          android:layout_width="fill_parent"
          android:layout_height="fill_parent"
          android:background="@drawable/midbox">
      </RelativeLayout>
      <RelativeLayout
          android:id="@+id/spec_overView"
          android:layout_width="fill_parent"
          android:layout_height="fill_parent"
          android:background="@drawable/Overview_Carmen3">
        <TextView
            android:id="@+id/EngLF1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="86dp"
            android:layout_marginLeft="55dp"
